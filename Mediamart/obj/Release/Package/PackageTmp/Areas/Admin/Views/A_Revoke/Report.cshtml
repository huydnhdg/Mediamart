<div class="card">
    <div class="card-header">
        <h3 class="card-title">Table database</h3>
    </div>
    <div class="card-body">
        <table class="table table-sm" id="table">
            <thead>
                <tr>
                    <th>Trạm đề xuất</th>
                    <th>Tổng</th>
                    <th>Mới tạo</th>
                    <th>Huỷ xác</th>
                    <th>Hoàn thành</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model.Station)
                {
                <tr>
                    <td>@item.Name</td>
                    <td>@item.all</td>
                    <td>@item.news</td>
                    <td>@item.cancel</td>
                    <td>@item.success</td>
                </tr>
                }
            </tbody>
        </table>
    </div>
    
    <div class="card-header">
